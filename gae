local farming = Window:CreateTab("autofarm", 4483362458) -- Title, Image

local allstage = farming:CreateSection("all stage farm",true/false) -- The 2nd argument is to tell if its only a Title and doesnt contain elements

farming:CreateToggle({
    Name = "enable",
    Info = "enables all stage autofarm", -- Speaks for itself, Remove if none.
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        settings.enabled = Value
        if settings.enabled then -- checks if autofarm is enabled
        autofarm() -- executes autofarm
    end
    if settings.enabled == false then -- checks if its not enabled
        settings.enabled = false 
end
    end,
 })

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()

local settings = {
    enabled = nil,
    speed = 24,
    tween_speed = 2,
    tween_easing_style = Enum.EasingStyle.Linear, -- linear, sine, back, quad, quart, quint, bounce, elastic, exponential, circular, cubic

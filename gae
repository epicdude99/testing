local hu = game.Players.LocalPlayer.Character.HumanoidRootPart

twens, tweninf = game:GetService("TweenService"), TweenInfo.new(settings.tween_speed, Enum.EasingStyle.Linear) -- tween info



function buyitem()
    while settings.auto_buy_items == true do wait()
local args = {
    [1] = settings.item_selected,
    [2] = settings.ammount_to_buy_item_in_shop
}

workspace.ItemBoughtFromShop:InvokeServer(unpack(args))
end
end



function autochestd()
    while settings.autochest == true do 
        wait(1)
local args = {
    [1] = settings.chest_selected,
    [2] = settings.chest_ammount
}
workspace.ItemBoughtFromShop:InvokeServer(unpack(args))
end
end


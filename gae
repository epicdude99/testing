--h.CFrame = CFrame.new(-55.7065, -358.74, 9492.36)


function autofarm()
    while settings.enabled == true  do-- inf loop
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(2, Enum.EasingStyle.Linear) -- tween info
    tweeninfo = TweenInfo.new(1.5, Enum.EasingStyle.Linear) -- tween info 
    tweenn = TweenInfo.new(3, Enum.EasingStyle.Linear) -- tween info
    h = game.Players.LocalPlayer.Character.HumanoidRootPart -- humanoid rootpart shortcut
        tweenInfoE = TweenInfo.new(2, Enum.EasingStyle.Linear) -- tween info
        AutofarmInfo = TweenInfo.new(settings.speed, Enum.EasingStyle.Linear) -- autofarm tween info
        game.Workspace.Gravity = 0.5-- reduces glitchiness
        local Tween = tweenService:Create(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), tweenInfo, {CFrame = CFrame.new(-54.377357482910156, 97.09781646728516, -164.42483520507812)})
        Tween:Play() -- plays the tween
        Tween.Completed:Wait()-- waits until the tween is completed
        local Tween2 = tweenService:Create(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), AutofarmInfo, {CFrame = CFrame.new(-100.3853988647461, 48.26547622680664, 8702.34375)})
        Tween2:Play() -- plays the tween
        Tween2.Completed:Wait() -- waits until the tween is completed
        local Tween3 = tweenService:Create(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), tweeninfo, {CFrame = CFrame.new(-100.36444854736328, -178.22418212890625, 8742.3349609375)})
        Tween3:Play() -- plays the tween
        Tween3.Completed:Wait() -- waits until tween is completed
        game.Workspace.Gravity = 196.2 -- sets gravity back to default
        h.CFrame = CFrame.new(-55.7065, -358.74, 9492.36) -- teleport to golden chest
        task.wait(17) -- wait 17 seconds
        if settings.notifications then --check if notifications are on 
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "farmed gold", -- Required
            Text = "farmed gold", -- Required
            Duration = 2
        })
        wait(3.3) -- wait 3.3 seconds
        if settings.notifications then 
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "repeating process", -- Required
                Text = "repeating process", -- Required
                Duration = 2
            })
    end
    end
     end
end    

if settings.enabled then -- checks if autofarm is enabled
    autofarm() -- executes autofarm
end
if settings.enabled == false then -- checks if its not enabled
    game.Players.LocalPlayer.Character.Humanoid.Health = 0 --destroys humanoid
end


local Window = Rayfield:CreateWindow({
    Name = "wayne.lua",
    LoadingTitle = "wayne.lua | Build a boat for treasure",
    LoadingSubtitle = "by samet.exe",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, -- Create a custom folder for your hub/game
       FileName = "wayneLua"
    },
    Discord = {
       Enabled = true,
       Invite = "sirius", -- The Discord invite code, do not include discord.gg/
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = false, -- Set this to true to use our key system
    KeySettings = {
       Title = "Sirius Hub",
       Subtitle = "Key System",
       Note = "Join the discord (discord.gg/sirius)",
       FileName = "SiriusKey",
       SaveKey = true,
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = "Hello"
    }
 })


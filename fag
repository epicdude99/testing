	local ModelDropdown = TeleportTab:AddDropdown({
		Name = "select item",
		Default = "choose a item..",
		Options = {}
	})
	
	-- Button to teleport the selected model
	local TeleportButton = TeleportTab:AddButton({
		Name = "teleport item",
		Callback = function()
			local selectedModelName = ModelDropdown.Value
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
			local model = workspace:FindFirstChild(selectedModelName)
	
			if model and model:IsA("Model") and model.PrimaryPart then
				model:SetPrimaryPartCFrame(humanoidRootPart.CFrame + Vector3.new(tpx,tpy,tpz))
				OrionLib:MakeNotification({
					Name = "teleport success",
					Content = "item teleported into your position",
					Image = "rbxassetid://4483345998",
					Time = 5
				})
			else
				OrionLib:MakeNotification({
					Name = "teleport error",
					Content = "selected model does not exist or doesn't have a primarypart.",
					Image = "rbxassetid://4483345998",
					Time = 5
				})
			end
		end
	})

	local Sections = TeleportTab:AddSection({
		Name = "stack bring items"
	})


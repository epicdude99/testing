					gunStatsModule.FireMode = "Auto"
					gunStatsModule.BulletSpeed = 200
					gunStatsModule.BulletDamage = 999
					gunStatsModule.BulletDistance = 999
					gunStatsModule.FireRate = 0
					gunStatsModule.ReloadTime = 0
					gunStatsModule.MaxBulletsAmount = 9999
					gunStatsModule.MinRecoilY = 0
					gunStatsModule.MaxRecoilY = 0
					gunStatsModule.MinRecoilX = 0
					gunStatsModule.MaxRecoilX = 0
					gunStatsModule.MinRecoilZ = 0
					gunStatsModule.MaxRecoilZ = 0
					gunStatsModule.FPFov = 80
					gunStatsModule.FPAimFov = 50
					gunStatsModule.ToAimSpeed = 0.1
					wait() -- Add a small delay to prevent server overload
				end
			end
		  end    
	})


-- a dusty trip
local KeySystemUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/MaGiXxScripter0/keysystemv2api/master/ui/xrer_mstudio45.lua"))()
KeySystemUI.New({
    ApplicationName = "wayne.lua", -- Your Key System Application Name
    Name = "wayne.lua", -- Your Script name
    Info = "Get Key For wayne.lua", -- Info text in the GUI, keep empty for default text.
    DiscordInvite = "", -- Optional.
    AuthType = "clientid" -- Can select verifycation with ClientId or IP ("clientid" or "ip")
})
repeat task.wait() until KeySystemUI.Finished() or KeySystemUI.Closed
if KeySystemUI.Finished() and KeySystemUI.Closed == false then
    print("Key verified, can load script")
	local esp = {jerrycan = nil, jcolor = Color3.fromRGB(255,255,255), carengine = nil, ecolor = Color3.fromRGB(255,255,255), yumyum = nil, fcolor = Color3.fromRGB(255,255,255), cooler = nil, ccolor = Color3.fromRGB(255,255,255), guns = nil, gcolor = Color3.fromRGB(255,255,255), monsters = nil, mcolor = Color3.fromRGB(255,255,255), caps = nil, cccolor = Color3.fromRGB(255,255,255)}
	local runservice = game:GetService("RunService")
	local tpx = 0
	local tpy = 0
	local tpz = 0
	local tpoffset = Vector3.new(tpx, tpy, tpz)
	local customfirerateValue = 16
	local customfirerate = nil

	local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

	local Window = OrionLib:MakeWindow({Name = "brainrot.lol | a dusty trip", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

	local function Noclip()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v:IsA("BasePart") and v.CanCollide == true then
            v.CanCollide = false
        end
    end
end

local function Clip() 
	for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do 
		if v:IsA("BasePart") and v.CanCollide == false then 
			v.CanCollide = true 
		end
	end
end

	function jerrycann()
		for i,v in pairs(game:GetService("Workspace"):GetChildren()) do 
			if v.Name == "GasCan" or v.Name == "gascan" or v.Name == "oilcan" or v.Name == "OilCan" or v.Name == "WaterCan" or v.Name == "watercan" or v.Name == "DieselCan" or v.Name == "dieselcan" then 
				local distance = (v.main.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
				local gas = v.main.tank.Value
		
				local partgui = Instance.new("BillboardGui")
				partgui.Parent = v
				partgui.AlwaysOnTop = true 
				partgui.Name = "itemespz"
				partgui.Size = UDim2.new(1,0,1,0)
		
				local frame = Instance.new("Frame")
				frame.Parent = partgui
				frame.BackgroundColor3 = esp.jcolor
				frame.BackgroundTransparency = 0.25
				frame.Size = UDim2.new(1,0,1,0)
				frame.BorderSizePixel = 0
				local corner = Instance.new("UICorner")
				corner.Parent = frame 
				local namegui = Instance.new("BillboardGui")
				namegui.Parent = v
				namegui.AlwaysOnTop = true 
				namegui.Name = "Name"
				namegui.Size = UDim2.new(1,0,1,0)
				namegui.SizeOffset = Vector2.new(0,1)
				local text = Instance.new("TextLabel")
				text.Parent = namegui
				text.Text = v.Name
				text.TextColor3 = esp.jcolor
				text.TextTransparency = 0.25
				text.BackgroundTransparency = 1
				text.TextScaled = true 
				text.Size = UDim2.new(1,0,1,0)
				text.Font = Enum.Font.Code
				text.Name = "Gayy"
				local distgui = Instance.new("BillboardGui")
				distgui.Parent = v 
				distgui.AlwaysOnTop = true 
				distgui.Name = "name"
				distgui.Size = UDim2.new(1,0,1,0)
				distgui.SizeOffset = Vector2.new(0,-1)
				local distext = Instance.new("TextLabel")
				distext.Parent = distgui 
				distext.Text = ""..math.floor(distance).."m"
				distext.TextColor3 = esp.jcolor
				distext.TextTransparency = 0.25
				distext.BackgroundTransparency = 1 
				distext.TextScaled = true 
				distext.Size = UDim2.new(1,0,1,0)
				distext.Font = Enum.Font.Code
				distext.Name = "gg"
				local gasgui = Instance.new("BillboardGui")
				gasgui.Parent = v 
				gasgui.AlwaysOnTop = true 
				gasgui.Name = "nm"
				gasgui.Size = UDim2.new(1,0,1,0)
				gasgui.SizeOffset = Vector2.new(1,0)
				local gastext = Instance.new("TextLabel")
				gastext.Parent = gasgui
				gastext.Text = "tank: "..gas
				gastext.TextColor3 = esp.jcolor
				gastext.TextTransparency = 0.25
				gastext.BackgroundTransparency = 1 
				gastext.TextScaled = true 
				gastext.Size = UDim2.new(1,0,1,0)
				gastext.Font = Enum.Font.Code
				gastext.Name = "gg"
			end
			end
		end


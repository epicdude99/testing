	local function refreshModelList()
		local models = {}
		for _, model in ipairs(workspace:GetChildren()) do
			if model:IsA("Model") and not models[model.Name] then
				models[model.Name] = true
			end
		end
	
		-- Sort model names alphabetically
		local modelNames = {}
		for name, _ in pairs(models) do
			table.insert(modelNames, name)
		end
		table.sort(modelNames)
	
		-- Refresh dropdown with updated options
		ModelDropdown:Refresh(modelNames, true) -- true to clear existing options
	
		-- Debug messages
		print("Model names:", table.concat(modelNames, ", "))
		print("Dropdown options:", table.concat(ModelDropdown.Options, ", "))
		print("Dropdown type:", type(ModelDropdown))
	end
	
	-- Function to continuously monitor changes in the workspace
	local function monitorWorkspace()
		while wait(5) do -- Adjust the interval as needed
			refreshModelList()
		end
	end
	
	-- Call refresh initially to populate the dropdown
	refreshModelList()
	
	-- Start monitoring the workspace for changes
	spawn(monitorWorkspace)

	game.Workspace.ChildAdded:Connect(function()
		if esp.jerrycan then 
			jerrycann()
		end
		if esp.carengine then 
			engineee()
		end
		if esp.yumyum then 
			queso()
		end
		if esp.cooler then 
			radiatorrr()
		end
		if esp.guns then 
			bangbang()
		end
		if esp.monsters then 
			monstahhh()
		end
		if esp.caps then 
			bottlecapp()
		end
	end)

	OrionLib:Init()
else
    print("Player closed the GUI.")
end

